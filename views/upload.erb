<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" type="text/css" href="/css/style.css" media="screen" />
  <script language="JavaScript" src="/javascript/formular_check.js"></script>
</head>

<body>

<div class="header">

</div>

<div class="nav">
  <ul>
    <li><a href=<%= url('/login') %>>Login</a></li>
    <li><a href=<%= url('/gallery') %>>Gallery</a></li>
    <li><a href=<%= url('/photosets') %>>Album</a></li>
    <li><a href=<%= url('/upload') %>>Upload</a></li>
  </ul>
</div>

<div class="content">
 
<form name="Form" action="/upload" method="post" enctype="multipart/form-data" onsubmit="return validateForm()">
  <label for="post_title">Title:</label><br />
  <input id="post_title" name="title" type="text" value="" size="35" maxlength="254"/>
  <br />

  <label for="post_file">Picture:</label><br />
  <input id="post_file" name="pictureLink" type="file" value="" size="35" maxlength="254"/>
  <br /> 

  <label for="post_description">Description:</label><br />
  <textarea id="post_description" name="description" rows="5"> </textarea>
  <br />
 
  <input type="submit" value="Hochladen!" />
</form> 

</div>


</body>

</html




